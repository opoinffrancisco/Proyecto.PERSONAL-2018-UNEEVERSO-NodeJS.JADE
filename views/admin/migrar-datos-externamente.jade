doctype html
html.login
  head
    meta(charset='UTF-8')
    title Uneeverso
    meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
    // bootstrap 3.0.2
    link(href='/css/bootstrap.min.css', rel='stylesheet', type='text/css')
    // font Awesome
    link(href='/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    // Theme style
    link(href='/css/AdminLTE.css', rel='stylesheet', type='text/css')
    // Theme style personalizado
    link(href='/css/style-login.css', rel='stylesheet', type='text/css')  
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
  body.login(style='background:#f9f9f900;')
    #login-box.form-box
      .header.skin-blue Uneeverso
      form#migrar_datos(method='post' action='/migrar-datos/importar' enctype='multipart/form-data')
        .body.bg-gray
          .form-group
            input.form-control(type='password',  id='key_public',name='key_public', placeholder='Token de control', value='', required='TRUE')
          .form-group
            input.form-control(type='file',  name='archivodb',id='archivodb',required='TRUE')
        .footer
          .form-group
            button.form-control.btn.bg-blue.btn-block(type='submit',  name='btn-importar',id='btn-importar',required='TRUE') Importar
    // jQuery 2.0.2
    script(src='/js/libs/jquery.min.js')
    // Bootstrap
    script(src='/js/bootstrap.min.js', type='text/javascript')
    // Utilidades - configuracion 
    script(src='/js/pages/configuracion/configuracion.js', type='text/javascript')
    // Utilidades - datos
    script(src='/js/pages/utilidades/datos.js', type='text/javascript')
